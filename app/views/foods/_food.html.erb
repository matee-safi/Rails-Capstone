<%= stylesheet_link_tag 'foods', media: 'all' %>

<div id="<%= dom_id food %>">
<tr>
<td><%= food.name %></td>
<td><%= food.measurement_unit %></td>
<td><%= food.price %></td>
<td><%= food.quantity %></td>
<td>
  <%= link_to 'Edit', edit_food_path(food) %>
  <%= link_to 'Delete', @food, method: :delete, data: { confirm: 'Are you sure?' } %>
</td>
</tr>
</div>
